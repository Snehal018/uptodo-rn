{
  "name": "uptodo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-install": "rm -rf node_modules yarn.lock ios/Pods ios/Podfile.lock && yarn install && npx pod-install",
    "bundle-android": "npx react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res/",
    "release-apk": "yarn bundle-android && rm -rf ./android/app/src/main/res/drawable-*  && cd android && ./gradlew assembleRelease && open ./app/build/outputs/apk/release"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.5.0",
